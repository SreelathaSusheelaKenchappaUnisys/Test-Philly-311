                          SaveAndServiceNotNeeded();
                      }   
                 }
                }
                
                 function Dumpster8() {  
                 if((j$('[id*=dump7]').val() == 'Construction') )  { 
                      if(confirm('If the dumpster that is blocking the sidewalk is enclosed inside a fence, there is no sanitation violation.')) {
                          SaveAndServiceNotNeeded();
                      }   
                 }
                }
                
               function Dumpster6() {  
                 if((j$('[id*=dump10]').val() == 'Yes') && (j$('[id*=dump9]').val() == 'Yes'))  { 
                      if(confirm('If the dumpster that is blocking the sidewalk is enclosed inside a fence, there is no sanitation violation.')) {
                          SaveAndServiceNotNeeded();
                      }   
                 }
                }
                
                
