                function curb(data) {
                if(j$('[id*=sanit2]').val() == 'No')  { 
                      if(confirm('The customer should call back when the violation is visible on the curb or driveway.')) {
                      SaveAndServiceNotNeeded();                       
                      }   
                 }
                }
                           
       function trash(data)    {
                
                if(data.value == "Yes") {
                if(confirm('This is the trash day for that location. If the trash is not picked up today, then report the sanitation violation.')) 
                     SaveAndServiceNotNeeded();
            }
         }
         
         function Vacant() {  
                 if(j$('[id*=sanit5]').val() == 'Yes')  { 
                      if(confirm('The system has changed the Case Record Type to Vacant Lot Clean-Up.')) {
                        j$('[id*=departmnt]').val('Community Life Improvement Program');
                        j$('[id*=caseRecordType]').val('Vacant Lot Clean-Up');
                         j$('[id*=SubjectForCase]').val('Vacant Lot Clean-Up');
                       j$('[id*=sreq]').val('Vacant Lot Clean-Up');
                     crts();                   
                      }   
                 }
                }
                
                function Active() {  
                 if((j$('[id*=sanit6]').val() == 'No') || (j$('[id*=sanit8]').val() == 'No') || (j$('[id*=sanit9]').val() == 'No') || (j$('[id*=sanit10]').val() == 'Yes')) { 
                      if(confirm('The system has changed the Case Record Type to Illegal Dumping.')) {
                       j$('[id*=caseRecordType]').val('Illegal Dumping');
                      j$('[id*=SubjectForCase]').val('Illegal Dumping');
                       j$('[id*=sreq]').val('Illegal Dumping');
                       rsChange();                      
                      }   
                 }
                }
                
                function Secure() {  
                 if(j$('[id*=sanit7]').val() == 'Yes')  { 
                      if(confirm('This is not a sanitation violation.')) {
                      SaveAndServiceNotNeeded();                       
                      }   
                 }
                }
                
                function private() {  
                 if(j$('[id*=sanit11]').val() == 'Yes')  { 
                      if(confirm('Private collection companies maintain alternate pickup dates.')) {
                      SaveAndServiceNotNeeded();                       
                      }   
                 }
                }
                
                function Dumpster1() {  
                 if((j$('[id*=dump1]').val() == 'Yes')){
                      if(confirm('To report a restaurant dumpster violation, contact the Health Department - Office of Food Protection at 215-685-7495.')) {
                      SaveAndServiceNotNeeded();                    
                      }   
                 }
                }
                
                function Dumpster2() {
                
                if(j$('[id*=dump2]').val() == 'Yes')
                {
                    if((j$('[id*=dump3]').val() == 'Yes')){
                  if(confirm('The system has changed the Case Record Type to Maintenance Residential or Commercial.')) {
                      j$('[id*=departmnt]').val('License & Inspections');                      
                      j$('[id*=caseRecordType]').val('Maintenance Residential or Commercial');
                       j$('[id*=SubjectForCase]').val('Maintenance Residential or Commercial');
                       crts();                     
                      }
                      }
                      
                else 
                      if(confirm('The system has changed the Case Record Type to Maintenance Residential or Commercial.')) {
                      j$('[id*=departmnt]').val('License & Inspections');                      
                      j$('[id*=caseRecordType]').val('Maintenance Residential or Commercial');
                      j$('[id*=SubjectForCase]').val('Maintenance Residential or Commercial');
                       crts();                     
                      }   
                 }
                }
                
                
                function Dumpster4() {  
