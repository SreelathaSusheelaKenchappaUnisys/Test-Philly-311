                      }   
                 }
                }
                
                
                
                function Dumpster5() {  
                 if((j$('[id*=dump7]').val() == 'Construction')  && (j$('[id*=dump6]').val() == 'Yes') && (j$('[id*=dump5]').val() == 'Yes') && (j$('[id*=dump8]').val() == 'Yes'))  { 
